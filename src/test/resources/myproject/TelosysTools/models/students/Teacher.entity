@DbTable(TEACHER)
@DbCatalog(TEST)
@DbSchema(STUDENTS)
Teacher {
  // attributes
  code : int { @Id @DbName(CODE) @DbType(INTEGER) @NotNull @NotEmpty };
  name : string { @DbName(NAME) @DbType("CHARACTER VARYING(40)") @Size(40) @MaxLen(40) };
  // links
  courseList : Course[] { @ManyToMany };
  studentList : Student[];
  studentList2 : Student[];
}
